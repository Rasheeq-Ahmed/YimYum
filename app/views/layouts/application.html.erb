<!DOCTYPE html>
<html>
  <head>
    <title>YimYum</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    
    <script type="text/javascript"> 
        window.logo = "<%= image_url("logo.png") %>"
        window.upload = "<%= image_url("upload.png") %>"
        window.uploadSymbol = '<%= image_url('upload_symbol.svg') %>'

    </script>
 
 
  </head>

  <body>
    <%= yield %>
  </body>
</html>
